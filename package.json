{
  "name": "FurrEverFriends",
  "version": "1.0.0",
  "description": "An app to teach you to write React",
  "main": "FurrEverFriends/src/App.js",
  "scripts": {
    "test": "jest --silent",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "format": "prettier --write \"FurrEverFriends/src/**/*.{js,jsx}\"",
    "lint": "eslint \"FurrEverFriends/src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel FurrEverFriends/src/index.html",
    "build": "parcel build FurrEverFriends/src/index.html"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!petfinder-client).+"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adelgado0723/FurrEverFriends.git"
  },
  "keywords": [
    "React",
    "JavaScript"
  ],
  "author": "Andy Delgado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adelgado0723/FurrEverFriends/issues"
  },
  "homepage": "https://github.com/adelgado0723/FurrEverFriends#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "google-maps-react": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-test-renderer": "^16.9.0"
  }
}
